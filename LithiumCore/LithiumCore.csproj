<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Authors>$(AssemblyName) </Authors>
    <BaseOutputPath>D:\[A] Code\C#\LithiumNukerV2\Compiled</BaseOutputPath>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="..\LithiumNukerV2\Settings.cs" Link="Settings.cs" />
    <Compile Include="..\LithiumNukerV2\WorkController.cs" Link="WorkController.cs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="Veylib">
      <HintPath>..\..\Veylib\Veylib\bin\Release\Veylib.dll</HintPath>
    </Reference>
  </ItemGroup>

</Project>
